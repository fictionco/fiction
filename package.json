{
  "name": "supereon",
  "type": "module",
  "version": "6.0.24",
  "private": true,
  "author": "Supereon",
  "license": "GPL-2.0-only",
  "main": "index.ts",
  "publishConfig": {
    "access": "restricted"
  },
  "engines": {
    "node": ">20.0.0"
  },
  "scripts": {
    "deploy": "flyctl deploy . --config ./fly/$DOMAIN.$APP.toml --build-target $DOMAIN-$APP --remote-only --detach --build-arg NPM_TOKEN=$NPM_TOKEN",
    "release": "npm exec -- fiction run release --patch --skip-tests --with-changes --tag",
    "bump": "npm exec -- fiction run release --patch --skip-tests --version-only",
    "push-release": "git checkout release && git rebase dev && git push --no-verify && git checkout dev",
    "test": "npm run lint:dev && npm run unit:dev",
    "test:pkgJson": "npx npmPkgJsonLint .",
    "fix-deps": "syncpack fix-mismatches",
    "changelog": "npm exec -- conventional-changelog -p angular -i CHANGELOG.md -s",
    "app:dev": "npm exec -w @fiction/www -- fiction run dev",
    "app:dev-r": "npm exec -w @fiction/www -- fiction run dev-r",
    "unit:ci": "npm exec -- vitest ci",
    "unit:build": "npm exec -- vitest run build -u --no-file-parallelism",
    "unit:dev": "npm exec -- vitest wip --reporter=hanging-process --reporter=verbose",
    "unit:e2e": "npm exec -- vitest e2e",
    "unit:all": "npm exec -- vitest wip && npm exec -- vitest ci && npm exec -- vitest build",
    "unit:dist": "npm exec -- vitest dist",
    "unit:coverage": "npm exec -- vitest run wip ci ui --coverage --ui -u",
    "types:ci": "npm exec -- vue-tsc",
    "types:dev": "npm exec -- vue-tsc --watch",
    "lint:dev": "npm exec -- eslint . --fix",
    "lint:view": "npx eslint-flat-config-viewer",
    "i:clear": "rm pnpm-lock.yaml && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "prep": "npm run i && npm run lint:dev",
    "i": "pnpm i && pnpm up -r && pnpm outdated -r && npm run i:lockfile",
    "i:lockfile": "pnpm i --lockfile-only",
    "i:full": "pnpm i:clear && npm run i",
    "docs:dev": "vitepress dev docs --host",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "workspaces": [
    "@fiction/*"
  ],
  "simple-git-hooks": {
    "pre-push": "npm run types:ci"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.1",
    "@esbuild-kit/esm-loader": "^2.6.5",
    "@fiction/core": "workspace:*",
    "@fiction/plugin-notify": "workspace:*",
    "@fiction/ui": "workspace:*",
    "@fiction/www": "workspace:*",
    "@vitejs/plugin-vue": "^5.0.4",
    "axios": "^1.6.8",
    "commander": "^12.0.0",
    "conventional-changelog-cli": "^4.1.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.4.5",
    "execa": "^8.0.1",
    "fake-indexeddb": "^5.0.2",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "node-fetch": "^3.3.2",
    "source-map-support": "^0.5.21",
    "textlint": "^14.0.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vite": "5.1.6",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.9.0",
    "@benasher44/eslint-plugin-implicit-dependencies": "^1.1.3",
    "@codspeed/vitest-plugin": "^3.1.0",
    "@eslint/eslintrc": "^3.0.2",
    "@flydotio/dockerfile": "^0.5.3",
    "@playwright/test": "^1.42.1",
    "@types/eslint__eslintrc": "^2.1.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitest/coverage-istanbul": "^1.4.0",
    "@vue/test-utils": "^2.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-eqeqeq-fix": "^1.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-vue": "^9.23.0",
    "happy-dom": "^14.3.1",
    "jsdom": "^24.0.0",
    "ngrok": "5.0.0-beta.2",
    "nodemon": "^3.1.0",
    "npm-package-json-lint": "^7.1.0",
    "playwright": "^1.42.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "sharp": "^0.33.2",
    "simple-git-hooks": "^2.11.0",
    "tslib": "^2.6.2",
    "vite-plugin-dts": "^3.7.3",
    "vitepress": "1.0.0-rc.45",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.7"
  },
  "pnpm": {
    "overrides": {}
  }
}
